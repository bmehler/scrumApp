<div class="container" style="margin-top:50px;">
  <div class="row">
    <div class="col-lg-12">
      <h1>Listing Product Backlog</h1>
        <ul class="list-group">
          <% @productbacklogs.each do |productbacklog| %>
            <li class="list-group-item">
              <%= productbacklog.name %>              
              <%= link_to "DELETE", { controller: "productbacklogs", action: "confirmDelete", id: productbacklog }, class: "btn btn-info btn-xs pull-right" %>
              <%= link_to "UPDATE", edit_productbacklog_path(productbacklog), { :class => "btn btn-info btn-xs pull-right" } %>
              <%= link_to 'SHOW', productbacklog_path(productbacklog), { :class => "btn btn-info btn-xs pull-right" } %>
            </li>
          <% end %>
        </ul>
        <%= link_to 'New Product Backlog', new_productbacklog_path, { :class => "btn btn-success"} %>
     </div>
   </div>
</div>
